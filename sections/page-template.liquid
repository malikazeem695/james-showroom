<section class="section template_{{ handle }}">
  <div
    class="
      container
      content
      page
    "
  >
  {% if handle contains 'accessibility-statement' %}
    {% else %}
    <div class="one-whole column">
      <h1>{{ page.title }}</h1>
    </div>
  {% endif %}

    {%
      render 'page-multi-column',
      content: page.content,
    %}
  </div>
</section>
